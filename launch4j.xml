<?xml version="1.0" encoding="UTF-8"?>
<launch4jConfig>
	<!-- <dontWrapJar>true</dontWrapJar> -->
	<headerType>gui</headerType>
	<jar>build/libs/StickyNotes.jar</jar>
	<outfile>build/launch4j/StickyNotes.exe</outfile>
	<errTitle></errTitle>
	<cmdLine></cmdLine>
	<chdir>.</chdir>
	<priority>idle</priority>
	<downloadUrl>http://java.com/download</downloadUrl>
	<supportUrl></supportUrl>
	<stayAlive>false</stayAlive>
	<restartOnCrash>false</restartOnCrash>
	<manifest></manifest>
	<icon>src/main/resources/icon.ico</icon>
	<jre>
		<path>windows-jre</path>
		<bundledJre64Bit>false</bundledJre64Bit> <!-- am I sure? -->
		<bundledJreAsFallback>false</bundledJreAsFallback>
		<minVersion></minVersion>
		<maxVersion></maxVersion>
		<!-- <jdkPreference>jdkOnly</jdkPreference> -->
		<runtimeBits>64/32</runtimeBits>
		<opt>-Dsun.java2d.dpiaware=true</opt>
	</jre>
	<versionInfo>
		<fileVersion>1.5.0.11</fileVersion>
		<txtFileVersion>1.5</txtFileVersion>
		<fileDescription>NoteBot - A clone of Microsoft Sticky Notes</fileDescription>
		<copyright>Federico Dossena</copyright>
		<productVersion>1.5.0.11</productVersion>
		<txtProductVersion>1.5</txtProductVersion>
		<productName>NoteBot</productName>
		<companyName>Federico Dossena</companyName>
		<internalName>StickyNotes</internalName>
		<originalFilename>StickyNotes.exe</originalFilename>
		<trademarks></trademarks>
		<language>ENGLISH_US</language>
	</versionInfo>
</launch4jConfig>
